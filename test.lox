class Desert {
    foo() {
        print "foo";
    }
}

class Cake < Desert {
    init() {
        this.flavor = "victoria sponge";
    }

    taste() {
        var adjective = "delicious";
        print "This " + this.flavor + " cake is " + adjective + "!";
    }
}

var cake = Cake();
cake.taste();
cake.flavor = "chocolate";
cake.taste();
cake.foo();